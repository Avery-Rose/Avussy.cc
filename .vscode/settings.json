{
  "files.exclude": {
    "amplify/.config": true,
    "amplify/**/*-parameters.json": true,
    "amplify/**/amplify.state": true,
    "amplify/**/transform.conf.json": true,
    "amplify/#current-cloud-backend": true,
    "amplify/backend/amplify-meta.json": true,
    "amplify/backend/awscloudformation": true,
    "node_modules": true
  },
  "discord.lowerDetailsEditing": "Repo {workspace}/{git_branch}",
  "discord.detailsEditing": "{file_name}",
  "editor.fontFamily": "'FiraCode Nerd Font', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "terminal.integrated.fontFamily": "Consolas, 'Courier New', monospace",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line",
  "editor.wordWrap": "bounded",
  "editor.minimap.scale": 1,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "mouseover",
  "editor.minimap.side": "right",
  "editor.minimap.size": "proportional",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultLocation": "view",
  "terminal.integrated.letterSpacing": 0,
  "terminal.integrated.lineHeight": 1,
  "emojisense.languages": {
    "markdown": true,
    "plaintext": {
      "markupCompletionsEnabled": false,
      "emojiDecoratorsEnabled": false
    },
    "scminput": true,
    "git-commit": true
  },
  "github.copilot.inlineSuggest.enable": true,
  "prettier.arrowParens": "always",
  "prettier.endOfLine": "crlf",
  "prettier.jsxSingleQuote": true,
  "prettier.jsxBracketSameLine": true,
  "prettier.requireConfig": true,
  "prettier.singleQuote": true,
  "prettier.useTabs": false,
  "errorLens.statusBarMessageTemplate": "",
  "errorLens.messageTemplate": "$source: $message ($code)",
  "errorLens.statusBarColorsEnabled": false,
  "errorLens.messageBackgroundMode": "line",
  "errorLens.fontWeight": "normal",
  "errorLens.fontStyleItalic": false,
  "eslint.alwaysShowStatus": false,
  "eslint.format.enable": true,
  "eslint.packageManager": "yarn",
  "eslint.run": "onType"
}
