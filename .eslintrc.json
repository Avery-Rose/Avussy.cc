{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "google", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "semi": ["error", "always"],
    "react/prop-types": "off",
    "camelcase": "warn",
    "require-jsdoc": "off",
    "no-console": "warn",
    "no-unused-vars": "warn",
    "n/no-callback-literal": "off"
  }
}
