{
  "name": "avussy.cc",
  "private": true,
  "main": "./src/index.js",
  "scripts": {
    "build": "yarn run lint && react-scripts build",
    "commit": "cz",
    "eject": "react-scripts eject",
    "preinstall": "npx only-allow yarn",
    "postinstall": "husky install",
    "lint": "yarn run lint:prettier && yarn run lint:fix && yarn run sort:package-json",
    "lint:fix": "eslint --ext .js,.ts.tsx,.jsx src/ --fix",
    "lint:prettier": "yarn run prettier --write src/",
    "prepare": "husky install",
    "sort:package-json": "yarn sort-package-json",
    "start": "yarn run lint && react-scripts start",
    "test": "react-scripts test"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.ts": "eslint --cache --fix",
    "*.tsx": "eslint --cache --fix",
    "*.jsx": "eslint --cache --fix",
    "*.json": "prettier --write",
    "*.md": "prettier --write",
    "*.yml": "prettier --write",
    "*.yaml": "prettier --write",
    "*.css": "prettier --write",
    "*.scss": "prettier --write",
    "*.html": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@aws-amplify/cli": "^8.2.0",
    "@commitlint/config-angular": "^17.0.0",
    "@commitlint/cz-commitlint": "^17.0.0",
    "@commitlint/prompt": "^17.0.0",
    "@commitlint/prompt-cli": "^17.0.0",
    "@mui/icons-material": "^5.8.0",
    "@mui/x-data-grid": "^5.11.1",
    "@nextui-org/react": "^1.0.0-beta.10",
    "@react-icons/all-files": "^4.1.0",
    "@sentry/react": "^7.0.0",
    "@sentry/tracing": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "aws-amplify": "^4.3.22",
    "commitizen": "^4.2.4",
    "csv-parse": "^5.1.0",
    "framer-motion": "^6.5.1",
    "global": "^4.4.0",
    "is-ci": "^3.0.1",
    "next-themes": "^0.2.0",
    "only-allow": "^1.1.0",
    "prettier-eslint": "^15.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "6",
    "react-scripts": "5.0.1",
    "smooth-scrollbar": "^8.7.4",
    "sort-package-json": "^1.57.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "autoprefixer": "^10.4.7",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "prettier": "2.6.2",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  }
}
